<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="avarage-time-create-work-order-table" author="Thiago Tierre">
        <createTable tableName="avarage_time_work_order">
            <column name="id" type="UUID">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="work_order_id" type="UUID">
                <constraints nullable="false"/>
            </column>
            <column name="start_time" type="TIMESTAMP WITHOUT TIME ZONE">
                <constraints nullable="false"/>
            </column>
            <column name="end_time" type="TIMESTAMP WITHOUT TIME ZONE">
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>